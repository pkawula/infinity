{
    "name": "ly/infinity",
    "description": "An experimental Symfony+Vue CRUD",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/framework-bundle": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/console": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/config": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/security-bundle": "^5.4",
        "doctrine/doctrine-bundle": "^2.5"
    },
    "require-dev": {
        "symfony/twig-bridge": "^5.4",
        "phpstan/phpstan": "^1",
        "doctrine/doctrine-fixtures-bundle": "^3"
    },
    "autoload": {
        "psr-4": {
            "Infinity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Infinity\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable"
}
